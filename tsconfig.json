{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": true,
		"esModuleInterop": true,
		"outDir": "./dist",
		"sourceMap": true,
		"strict": true,
		"baseUrl": "./",
		"paths": {
			"@point/*": ["lib/point/*"],
			"@bezier/*": ["lib/bezier/*"],
			"@animator/*": ["lib/animator/*"],
			"@util/*": ["lib/util/*"]
		}
	}
}
